{
  "name": "nebula-rag-dashboard",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:ha": "node ./scripts/dev-ha.mjs",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:prod": "npm run build && echo 'Dashboard built to ../src/NebulaRAG.AddonHost/wwwroot'",
    "test:visual": "playwright test",
    "test:visual:update": "playwright test --update-snapshots"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.7",
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.4.5",
    "vite": "^5.1.3"
  }
}
