{
  "name": "Nebula RAG",
  "version": "0.2.39",
  "slug": "nebula_rag",
  "description": "NebulaRAG long-running add-on with Nebula web UI and MCP endpoint.",
  "url": "https://github.com/MarkBovee/NebulaRAG/tree/main/nebula-rag",
  "stage": "stable",
  "startup": "services",
  "boot": "auto",
  "init": false,
  "ingress": true,
  "ingress_port": 8099,
  "panel_icon": "mdi:star-four-points-outline",
  "panel_title": "Nebula RAG",
  "webui": "http://[HOST]:[PORT:8099]",
  "arch": [
    "amd64",
    "aarch64"
  ],
  "ports": {
    "8099/tcp": 8099
  },
  "ports_description": {
    "8099/tcp": "Nebula RAG web UI and MCP endpoint"
  },
  "map": [
    "share:rw",
    "addon_config:rw"
  ],
  "options": {
    "default_index_path": "/share",
    "path_base": "/nebula",
    "database": {
      "host": "homeassistant.local",
      "port": 5432,
      "name": "nebularag",
      "username": "postgres",
      "password": "",
      "ssl_mode": "Prefer"
    },
    "telemetry": {
      "otlp_endpoint": ""
    }
  },
  "schema": {
    "default_index_path": "str?",
    "path_base": "str?",
    "database": {
      "host": "str",
      "port": "int(1,65535)",
      "name": "str",
      "username": "str",
      "password": "password",
      "ssl_mode": "list(Disable|Allow|Prefer|Require)"
    },
    "telemetry": {
      "otlp_endpoint": "str?"
    }
  }
}
